{"ast":null,"code":"import \"core-js/modules/es.number.constructor.js\";\nimport { VDialog, VCard, VCardTitle, VSpacer, VBtn, VIcon, VDivider, VCardActions, VOverlay, VProgressCircular } from 'vuetify/lib';\nimport UBtn from '../UBtn';\nexport default {\n  components: {\n    VDialog: VDialog,\n    VCard: VCard,\n    VCardTitle: VCardTitle,\n    VSpacer: VSpacer,\n    VBtn: VBtn,\n    VIcon: VIcon,\n    VDivider: VDivider,\n    VCardActions: VCardActions,\n    VOverlay: VOverlay,\n    VProgressCircular: VProgressCircular,\n    UBtn: UBtn\n  },\n  props: {\n    // show/hide dialog\n    value: {\n      type: Boolean,\n      \"default\": false\n    },\n    // dialog window title\n    title: {\n      type: String,\n      \"default\": ''\n    },\n    // custom close buton label, default set as undefined to enable dynamic translation\n    cancelLabel: {\n      type: String,\n      \"default\": undefined\n    },\n    // custom action button label, default set as undefined to enable dynamic translation\n    submitLabel: {\n      type: String,\n      \"default\": undefined\n    },\n    // boolean to disable/enable submit button\n    submitDisabled: {\n      type: Boolean,\n      \"default\": false\n    },\n    // dialog width as a number or percentage\n    width: {\n      type: [String, Number],\n      \"default\": 650\n    },\n    // dialog width as a number or percentage\n    height: {\n      type: [String, Number],\n      \"default\": 'auto'\n    },\n    // boolean to show progress while pending action\n    showProgress: {\n      type: Boolean,\n      \"default\": false\n    },\n    // info or error message to show\n    message: {\n      type: String,\n      \"default\": null\n    }\n  },\n  data: function data() {\n    return {\n      dialog: this.value\n    };\n  },\n  watch: {\n    value: function value(dialog) {\n      var _this = this;\n      // close on ESC key\n      var _handler = function handler(e) {\n        if (e.keyCode === 27) {\n          window.removeEventListener('keydown', _handler);\n          _this.$emit('cancel');\n        }\n      };\n      if (dialog) {\n        window.addEventListener('keydown', _handler);\n      }\n\n      // mutate data as it is used as v-model for v-dialog and cannot mutate the props\n      this.dialog = dialog;\n    }\n  }\n};","map":{"version":3,"names":["VDialog","VCard","VCardTitle","VSpacer","VBtn","VIcon","VDivider","VCardActions","VOverlay","VProgressCircular","UBtn","components","props","value","type","Boolean","title","String","cancelLabel","undefined","submitLabel","submitDisabled","width","Number","height","showProgress","message","data","dialog","watch","_this","handler","e","keyCode","window","removeEventListener","$emit","addEventListener"],"sources":["node_modules/vuntangle/src/components/UDialogComplex/UDialogComplex.vue"],"sourcesContent":["<template>\n  <v-dialog v-model=\"dialog\" :width=\"width\" persistent content-class=\"modal-border\">\n    <v-card :disabled=\"showProgress\">\n      <v-card-title class=\"px-4 py-1 aristaMediumBlue white--text\">\n        {{ title }}\n        <v-spacer />\n        <v-btn icon @click=\"$emit('cancel')\"><v-icon color=\"white\">mdi-close</v-icon></v-btn>\n      </v-card-title>\n      <v-divider />\n\n      <div class=\"pa-4\" style=\"min-height: 100px\">\n        <!-- default slot -->\n        <slot></slot>\n      </div>\n\n      <v-card-actions class=\"px-4 pb-4 pt-0\">\n        <!-- info or error message to display -->\n        <span v-if=\"message\" style=\"color: #555\"> <v-icon small>mdi-information-outline</v-icon> {{ message }} </span>\n        <v-spacer />\n\n        <u-btn color=\"default\" class=\"mr-2\" @click=\"$emit('cancel')\">\n          <span :class=\"`${$vuetify.theme.dark ? 'white--text' : ''}`\">{{ cancelLabel || $t('cancel') }}</span>\n        </u-btn>\n        <u-btn :disabled=\"submitDisabled\" @click=\"$emit('submit')\">{{ submitLabel || $t('ok') }}</u-btn>\n\n        <!-- add an extra slot allowing to set extra actions -->\n        <div v-if=\"$slots.extraActions\" class=\"ml-4\">\n          <slot name=\"extraActions\"></slot>\n        </div>\n      </v-card-actions>\n      <v-overlay :value=\"showProgress\" absolute color=\"rgba(255, 255, 255, 0.66)\" opacity=\"1\" class=\"text-center\">\n        <v-progress-circular indeterminate size=\"32\" color=\"aristaMediumBlue\" />\n      </v-overlay>\n    </v-card>\n  </v-dialog>\n</template>\n<script>\n  import {\n    VDialog,\n    VCard,\n    VCardTitle,\n    VSpacer,\n    VBtn,\n    VIcon,\n    VDivider,\n    VCardActions,\n    VOverlay,\n    VProgressCircular,\n  } from 'vuetify/lib'\n  import UBtn from '../UBtn'\n\n  export default {\n    components: {\n      VDialog,\n      VCard,\n      VCardTitle,\n      VSpacer,\n      VBtn,\n      VIcon,\n      VDivider,\n      VCardActions,\n      VOverlay,\n      VProgressCircular,\n      UBtn,\n    },\n    props: {\n      // show/hide dialog\n      value: {\n        type: Boolean,\n        default: false,\n      },\n      // dialog window title\n      title: {\n        type: String,\n        default: '',\n      },\n      // custom close buton label, default set as undefined to enable dynamic translation\n      cancelLabel: {\n        type: String,\n        default: undefined,\n      },\n      // custom action button label, default set as undefined to enable dynamic translation\n      submitLabel: {\n        type: String,\n        default: undefined,\n      },\n      // boolean to disable/enable submit button\n      submitDisabled: {\n        type: Boolean,\n        default: false,\n      },\n      // dialog width as a number or percentage\n      width: {\n        type: [String, Number],\n        default: 650,\n      },\n      // dialog width as a number or percentage\n      height: {\n        type: [String, Number],\n        default: 'auto',\n      },\n      // boolean to show progress while pending action\n      showProgress: {\n        type: Boolean,\n        default: false,\n      },\n      // info or error message to show\n      message: {\n        type: String,\n        default: null,\n      },\n    },\n    data() {\n      return {\n        dialog: this.value,\n      }\n    },\n    watch: {\n      value(dialog) {\n        // close on ESC key\n        const handler = e => {\n          if (e.keyCode === 27) {\n            window.removeEventListener('keydown', handler)\n            this.$emit('cancel')\n          }\n        }\n        if (dialog) {\n          window.addEventListener('keydown', handler)\n        }\n\n        // mutate data as it is used as v-model for v-dialog and cannot mutate the props\n        this.dialog = dialog\n      },\n    },\n  }\n</script>\n"],"mappings":";AAqCA,SACAA,OAAA,EACAC,KAAA,EACAC,UAAA,EACAC,OAAA,EACAC,IAAA,EACAC,KAAA,EACAC,QAAA,EACAC,YAAA,EACAC,QAAA,EACAC,iBAAA,QACA;AACA,OAAAC,IAAA;AAEA;EACAC,UAAA;IACAX,OAAA,EAAAA,OAAA;IACAC,KAAA,EAAAA,KAAA;IACAC,UAAA,EAAAA,UAAA;IACAC,OAAA,EAAAA,OAAA;IACAC,IAAA,EAAAA,IAAA;IACAC,KAAA,EAAAA,KAAA;IACAC,QAAA,EAAAA,QAAA;IACAC,YAAA,EAAAA,YAAA;IACAC,QAAA,EAAAA,QAAA;IACAC,iBAAA,EAAAA,iBAAA;IACAC,IAAA,EAAAA;EACA;EACAE,KAAA;IACA;IACAC,KAAA;MACAC,IAAA,EAAAC,OAAA;MACA;IACA;IACA;IACAC,KAAA;MACAF,IAAA,EAAAG,MAAA;MACA;IACA;IACA;IACAC,WAAA;MACAJ,IAAA,EAAAG,MAAA;MACA,WAAAE;IACA;IACA;IACAC,WAAA;MACAN,IAAA,EAAAG,MAAA;MACA,WAAAE;IACA;IACA;IACAE,cAAA;MACAP,IAAA,EAAAC,OAAA;MACA;IACA;IACA;IACAO,KAAA;MACAR,IAAA,GAAAG,MAAA,EAAAM,MAAA;MACA;IACA;IACA;IACAC,MAAA;MACAV,IAAA,GAAAG,MAAA,EAAAM,MAAA;MACA;IACA;IACA;IACAE,YAAA;MACAX,IAAA,EAAAC,OAAA;MACA;IACA;IACA;IACAW,OAAA;MACAZ,IAAA,EAAAG,MAAA;MACA;IACA;EACA;EACAU,IAAA,WAAAA,KAAA;IACA;MACAC,MAAA,OAAAf;IACA;EACA;EACAgB,KAAA;IACAhB,KAAA,WAAAA,MAAAe,MAAA;MAAA,IAAAE,KAAA;MACA;MACA,IAAAC,QAAA,YAAAA,QAAAC,CAAA;QACA,IAAAA,CAAA,CAAAC,OAAA;UACAC,MAAA,CAAAC,mBAAA,YAAAJ,QAAA;UACAD,KAAA,CAAAM,KAAA;QACA;MACA;MACA,IAAAR,MAAA;QACAM,MAAA,CAAAG,gBAAA,YAAAN,QAAA;MACA;;MAEA;MACA,KAAAH,MAAA,GAAAA,MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externa